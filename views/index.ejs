	<![endif]-->
	<script type="text/javascript">
		window.onload = function () {
			// fix for windows 8
			if (navigator.appVersion.indexOf("Windows NT 6.2") != -1)
				document.head.innerHTML += '<link rel="stylesheet" type="text/css" href="/template/pages/css/windows.chrome.fix.css" />'
		};
		window.App = {};
		window.App.auth = <%- JSON.stringify(locals) %>.auth;
		window.App.title = <%- JSON.stringify(locals) %>.title;
	</script>
</head>
<body class="fixed-header dashboard <!--menu-pin-->">
<nav class="page-sidebar" data-pages="sidebar">
	<div class="sidebar-header">
		<img src="/image/logo.png" alt="logo" class="brand" data-src="/image/logo.png" data-src-retina="/image/logo.png" width="120" height="30">
		<div class="sidebar-header-controls">
			<button type="button" class="btn btn-xs sidebar-slide-toggle btn-link m-l-5">
				<!--<i class="fa fa-angle-down fs-16"></i>-->
			</button>
			<button type="button" class="btn btn-link visible-lg-inline" data-toggle-pin="sidebar"><i class="fa fs-12"></i>
			</button>
		</div>
	</div>
	<div class="sidebar-menu">
		<ul class="menu-items">
			<% if (locals.auth.user_name == "admin") { %>
			<li class="" todo="events">
				<a href="javascript:void(0);">
					<span class="title" title="Keadaan Darurat">Tombol Panik</span>
				</a>
				<span class="icon-thumbnail">
					<i class="fa fa-rss"></i>
				</span>
			</li>
			<li class="" todo="monitor">
				<a href="javascript:void(0);">
					<span class="title" title="Monitoring Kegiatan">Monitoring</span>
				</a>
				<span class="icon-thumbnail">
					<i class="pg-map"></i>
				</span>
			</li>
			<li class="" todo="schedule">
				<a href="javascript:void(0);">
					<span class="title" title="Jadwal Kegiatan">Kegiatan</span>
				</a>
				<span class="icon-thumbnail">
					<i class="fa fa-list"></i>
				</span>
			</li>
			<% } %>
			<li class="" todo="attendance">
				<a href="javascript:void(0);">
					<span class="title" title="Isi Presensi">Kehadiran</span>
				</a>
				<span class="icon-thumbnail">
					<i class="pg-calender"></i>
				</span>
			</li>
			<% if (locals.auth.user_name == "admin") { %>
			<li class="" todo="users">
				<a href="javascript:void(0);">
					<span class="title" title="Panitia">Panitia</span>
				</a>
				<span class="icon-thumbnail">
					<i class="fa fa-users"></i>
				</span>
			</li>
			<li class="">
				<a href="javascript:;">
					<span class="title">Laporan</span>
					<span class="arrow open"></span>
				</a>
				<span class="icon-thumbnail"><i class="fa fa-fax"></i></span>
				<ul class="sub-menu" style="display: block;">
					<li class="" todo="report_jambore">
						<a href="javascript:void(0);" title="Jambore Nasional">Jambore</a>
						<span class="icon-thumbnail">J</span>
					</li>
					<li class="" todo="report_camp">
						<a href="javascript:void(0);" title="Camping">Camp</a>
						<span class="icon-thumbnail">C</span>
					</li>
					<li class="" todo="report_activity">
						<a href="javascript:void(0);" title="Activitaas Kegiatan">Kegiatan</a>
						<span class="icon-thumbnail">K</span>
					</li>
				</ul>
			</li>
			<% } %>
		</ul>
		<div class="clearfix"></div>
	</div>
</nav>
<div class="page-container">
	<div class="header">
		<div class="pull-left full-height visible-sm visible-xs">
			<div class="sm-action-bar">
				<a href="#" class="btn-link toggle-sidebar" data-toggle="sidebar">
					<span class="icon-set menu-hambuger"></span>
				</a>
			</div>
		</div>
		<div class="pull-right full-height visible-sm visible-xs">
			<div class="sm-action-bar">
				<a href="#" class="btn-link" data-toggle="quickview" data-toggle-element="#quickview">
					<span class="icon-set menu-hambuger-plus"></span>
				</a>
			</div>
		</div>
		<div class=" pull-left sm-table">
			<div class="header-inner">
				<style>
					.header .brand {
						vertical-align: middle;
						width: 232px;
						text-align: right;
					}
					.alert span {
						cursor: pointer;
					}
				</style>
				<div class="brand inline">
					<img style="margin-right: 15px;" src="/image/logo.png" alt="logo" data-src="/image/logo.png" data-src-retina="/image/logo.png"
					     width="120" height="30">
				</div>
				<ul class="notification-list no-margin hidden-sm hidden-xs b-grey b-l b-r no-style p-l-30 p-r-20">
					<li class="p-r-15 inline">
						<a href="#" class="icon-set globe-fill"></a>
					</li>
				</ul>
			</div>
		</div>
		<div class=" pull-right">
			<div class="visible-lg visible-md m-t-10">
				<div class="pull-left p-r-10 p-t-10 fs-16 font-heading">
					<span class="text-master"><%- locals.auth.name %></span>
				</div>
				<div class="dropdown pull-right">
					<a href="/logout" class="clearfix">
						<span class="thumbnail-wrapper d32 circular inline m-t-5">
	                        <img src="/image/logout.png" alt="" data-src="/image/logout.png" data-src-retina="/image/logout.png" width="32" height="32">
		                </span>
					</a>
				</div>
			</div>
		</div>
	</div>
	<div class="page-content-wrapper">
		<div class="content sm-gutter">
			<div class="container-fluid p-r-15 p-l-15 p-t-15 p-b-10 sm-padding-10">
				<div id="content" class="row">
					<!-- CONTENT GOES HERE.. -->
				</div>
			</div>
		</div>
	</div>
</div>
